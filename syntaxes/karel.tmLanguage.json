			"captures": {
				"1": { "name": "keyword.control.conditional.karel" }
			  }
		},
		{
			"comment": "Karel signals",
			"match": "\\b(DOUT|DIN|GIN|GOUT)\\b",
			"captures": {
				"1": { "name": "support.function.karel" }
			  }
		},
		{
			"comment": "Karel operator",
