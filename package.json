{
    "name": "karel-syntax",
    "displayName": "KAREL-Language-Support",
    "description": "Fanuc KAREL Syntax Support",
    "version": "1.0.0",
    "publisher": "fabi/andymtorres",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
        {
            "id": "fanuc-karel",
            "aliases": ["Fanuc-Karel(.kl)", "fanuc-karel"],
            "extensions": [".kl", ".klh", ".klt"],
            "configuration": "./language-configuration.json"
        },
        {
            "id": "CM",
            "aliases": ["CM(.CM)"],
            "extensions": [".cm", ".CM"],
            "configuration": "./language-configuration.json"
        },
        {
            "id": "ftx",
            "aliases": ["FTX(.FTX)"],
            "extensions": [".ftx", ".FTX"],
            "configuration": "./language-configuration.json"
        },
        {
            "id": "etx",
            "aliases": ["ETX(.ETX)"],
            "extensions": [".etx", ".ETX"],
            "configuration": "./language-configuration.json"
        },
        {
            "id": "utx",
            "aliases": ["UTX(.UTX)"],
            "extensions": [".utx", ".UTX"],
            "configuration": "./language-configuration.json"
        }
    ],
        "grammars": [
        {
            "language": "karel",
            "scopeName": "source.kl",
            "path": "./syntaxes/fanuc-karel.tmLanguage.json"
        },
        {
            "language": "CM",
            "scopeName": "source.CM",
            "path": "./syntaxes/cm.tmLanguage.json"
        },
        {
            "language": "ftx",
            "scopeName": "source.ftx",
            "path": "./syntaxes/ftx.tmLanguage.json"
        },
        {
            "language": "etx",
            "scopeName": "source.etx",
            "path": "./syntaxes/etx.tmLanguage.json"
        },
        {
            "language": "utx",
            "scopeName": "source.utx",
            "path": "./syntaxes/utx.tmLanguage.json"
        }
    ],
        "snippets":[{
                "language": "karel",
                "path": "./snippets/karel.json"
        },
        {
                "language": "fanuc-karel",
                "path": "./snippets/fanuc-karel.json"
        }
    ],
        "themes": [
        {
            "label": "FANUC_Theme",
            "id": "karel",
            "uiTheme": "vs-dark",
            "path": "./themes/FANUC_theme-color-theme.json"
        }
    ]
    }
}
